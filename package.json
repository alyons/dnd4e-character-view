    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.1",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.5.0",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.5.0",
    "react-hot-loader": "^4.8.0",
    "redux-mock-store": "^1.5.3",
    "style-loader": "^0.23.1",
    "svg-inline-loader": "^0.8.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "aws-sdk": "^2.471.0",
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "react": "^16.8.4",
    "react-bootstrap": "^1.0.0-beta.9",
    "react-dom": "^16.8.4",
    "react-google-login": "^5.0.2",
    "react-inlinesvg": "^0.8.4",
    "react-redux": "^6.0.1",
    "react-tabs": "^3.0.0",
    "react-transition-group": "^4.0.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "build": "webpack",
    "start": "webpack-dev-server --hot --open --mode development",
    "test": "jest",
    "test:watch": "npm test -- --watch"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/index.js",
      "!**/LogIn.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "moduleNameMapper": {
      "\\.(ico|svg)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
